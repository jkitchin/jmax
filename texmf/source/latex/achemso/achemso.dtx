  \else
    \addpenalty\@secpenalty
    \addvspace\@tempskipa
  \fi
  \@ifstar{%
    \@ssect{#3}{#4}{#5}{#6}%
  }{%
    \@ssect{#3}{#4}{#5}{#6}%
  }%
}
